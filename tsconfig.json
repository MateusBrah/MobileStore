{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "jsx": "preserve",
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "module": "esnext",
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
